{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/student/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/student/(.*)",
      "dest": "server.js"
    }
  ]
}
